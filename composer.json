{
    "name": "aic/data-service-collections",
    "description": "A data service for the AIC data hub.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/data-hub-foundation.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.8.*",
        "aic/data-hub-foundation": "dev-master",
        "laravel/tinker": "^1.0",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/psr-http-message-bridge": "^1.1",
        "sentry/sentry-laravel": "^1.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
